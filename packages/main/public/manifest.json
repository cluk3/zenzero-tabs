{
  "name": "zenzero",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A plain text description",
  "icons": {},
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src chrome://favicon;",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "author": "Luca Campli"
}